#4
uwu=int(input("ingrese un numero"))
unu=int(input("ingrese otro numero"))
if uwu>unu:
    print("a es mayor que b")
if uwu<unu:
    print("a es menor que b")
else:
    print("a es igual que b")

#5    
num=int(input("ingrese un numero"))
if num%2==0:
    print("el numero es par")
else:
    print("el numero es impar")

#6
numero = int(input("Ingrese un número del 1 al 7: "))
if 1 <= numero <= 7:
        if numero == 1:
        dia = "Lunes"
    elif numero == 2:
        dia = "Martes"
    elif numero == 3:
        dia = "Miércoles"
    elif numero == 4:
        dia = "Jueves"
    elif numero == 5:
        dia = "Viernes"
    elif numero == 6:
        dia = "Sábado"
    else:
        dia = "Domingo"
        
print(f"El número {numero} corresponde a {dia}.")
else:
     print("Número fuera de rango. Por favor, ingrese un número del 1 al 7.")
     
#7
puntuacion = float(input("ingrese la puntuación del estudiante: "))
if 80 <= puntuacion <= 100:
    clasificacion = "A"
elif 70 <= puntuacion <= 89:
    clasificacion = "B"
elif 60 <= puntuacion <= 69:
    clasificacion = "C"
elif 50 <= puntuacion <= 59:
    clasificacion = "D"
elif 0 <= puntuacion <= 49:
    clasificacion = "F"
else:
    clasificacion = "puntuación fuera de rango. ingrese una puntuación válida."
    
print(f"clasificación según su puntuación: {clasificacion}")  

#8
edad=int(input("ingrese la edad del cliente: "))
if edad<4:
    precio=0
elif 4<=edad<=18:
    precio=5
else
    precio=10
    
precio(f"el precio de la entrada es: ${precio}")

#9
edad=int(input("ingrese su edad: "))
ingresos_mensuales=float(input("ingrese sus ingresos mensuales: $")

if edad>18 && ingresos_mensuales >= 100000:
    print("tenes que pagar el impuesto")
else:
    print("no tenes que pagar nada")
    
     



    

    
    

    
    
    